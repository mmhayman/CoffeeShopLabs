<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.test.models.UsersEntity" table="users" schema="GCcoffeeshop">
        <id name="userId">
            <column name="userID" sql-type="int(10) unsigned"/>
        </id>
        <property name="firstName">
            <column name="firstName" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="lastName">
            <column name="lastName" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="phoneNumber">
            <column name="phoneNumber" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="gender">
            <column name="gender" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="mailingList">
            <column name="mailingList" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
        <property name="preferences">
            <column name="preferences" sql-type="varchar(45)" length="45" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>